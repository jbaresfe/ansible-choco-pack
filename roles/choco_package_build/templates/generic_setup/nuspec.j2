<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>{{ choco_package_build_package_name }}</id>
    <version>{{ choco_package_build_object.version }}</version>
    <title>{{ choco_package_build_package_name }}</title>
    <authors>Anonymous</authors>
    <owners>Anonymous</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>{{ choco_package_build_package_name }}</description>
    <summary>{{ choco_package_build_package_name }} Deployment</summary>
    <tags>{{ choco_package_build_package_name }} SPACE_SEPARATED</tags>
    <dependencies>
      <dependency id="chocolatey-core.extension" version="1.1.0" />
{% for key, value in choco_package_build_object.choco_dependencies | dictsort(true)  %}
      <dependency id="{{ key }}" version="{{ value }}" />
{% endfor %}
    </dependencies>
  </metadata>
</package>
